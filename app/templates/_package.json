{
  "name": "<%= projectname %>",
  "version": "1.0.0",
  "description": "<%= description %>",
  "main": "index.js",
  "scripts": {
    "start": "foreman start",
    "build": "npm run build:app",
    "build:webpack": "node ./node_modules/webpack/bin/webpack.js --verbose --colors --display-error-details",
    "build:app": "npm run build:webpack -- --config webpack.prod.config.js",
    "lint:js": "./node_modules/eslint/bin/eslint.js src --ext .jsx,.js",
    "lint:css": "./node_modules/stylelint/dist/cli.js src/scss/*.scss src/scss/*/*.scss --syntax scss",
    "lint": "npm run lint:css && npm run lint:js",
    "watch": "npm run start & webpack-dev-server --inline --progress --color --config",
    "postinstall": "npm run build",
    "test": "mocha --compilers js:babel-core/register --require ./test/setup.js 'test/**/*spec.js'",
    "test:watch": "npm run test -- --watch"
  },
